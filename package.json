{
	"name": "@tuplo/datecalc",
	"description": "Perform date calculations given a simple string pattern",
	"version": "0.0.0-development",
	"repository": "git@github.com:tuplo/datecalc.git",
	"author": "Rui Costa",
	"license": "MIT",
	"keywords": [
		"date"
	],
	"files": [
		"cjs/index.js",
		"dist/index.js",
		"dist/index.d.ts"
	],
	"type": "module",
	"main": "./dist/index.js",
	"exports": {
		".": [
			{
				"import": "./dist/index.js",
				"require": "./dist/index.cjs",
				"default": "./dist/index.js"
			},
			"./dist/index.js"
		]
	},
	"types": "dist/index.d.ts",
	"engines": {
		"node": ">=14"
	},
	"devDependencies": {
		"@tuplo/shell": "1.2.0",
		"@types/node": "20.4.1",
		"@typescript-eslint/eslint-plugin": "6.0.0",
		"@typescript-eslint/parser": "6.0.0",
		"esbuild": "0.18.11",
		"eslint": "8.44.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-import": "2.27.5",
		"nyc": "15.1.0",
		"prettier": "3.0.0",
		"tsx": "3.12.7",
		"typescript": "5.1.6"
	},
	"scripts": {
		"build": "tsx sh/build.ts",
		"coverage": "tsx sh/coverage.ts",
		"format": "prettier --write src",
		"lint:ts": "tsc --noEmit",
		"lint": "eslint src/**/*.ts",
		"test:ci": "node --test --loader tsx src/*.test.ts",
		"test": "node --test --watch --test-reporter spec --loader tsx src/*.test.ts"
	}
}
